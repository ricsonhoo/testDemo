<template name="pickerTemp">
  <view class='rui-form-li'>
    <view class='rui-form-li-input0'>
      <text class='rui-icon-ps' style='background:url(data:image/png;base64,) no-repeat center center/100% 100%;'></text>
      <picker bindchange="changeList" name="{{attr}}"  value="{{index}}" data-list="{{attr}}" range="{{array}}">
        {{array[index]}}
      </picker>
    </view>
  </view>
</template>